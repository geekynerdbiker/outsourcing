<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="315.0" prefWidth="503.0" spacing="20.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="healthcare.HomeController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <Label prefHeight="51.0" prefWidth="265.0" text="국민 건강 체크 프로그램">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <HBox alignment="CENTER" prefHeight="76.0" prefWidth="249.0">
      <Button fx:id="hospitalBtn" mnemonicParsing="false" onAction="#onHospitalButtonClick" prefHeight="100.0"
              prefWidth="150.0" text="검진 기관 찾기">
         <HBox.margin>
            <Insets right="30.0"/>
         </HBox.margin>
      </Button>
      <Button fx:id="symptomBtn" mnemonicParsing="false" onAction="#onSymptomButtonClick" prefHeight="100.0"
              prefWidth="150.0" text="검진 정보"/>
   </HBox>
   <HBox alignment="CENTER" prefHeight="76.0" prefWidth="249.0">
      <Button fx:id="checkBtn" mnemonicParsing="false" onAction="#onCheckButtonClick" prefHeight="100.0"
              prefWidth="150.0" text="건강 체크">
         <HBox.margin>
            <Insets right="30.0"/>
         </HBox.margin>
      </Button>
      <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#onReturnButtonClick" prefHeight="100.0"
              prefWidth="150.0" text="종료"/>
   </HBox>
</VBox>

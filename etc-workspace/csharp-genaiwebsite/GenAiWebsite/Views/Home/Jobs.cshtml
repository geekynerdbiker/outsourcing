@{
    ViewData["Title"] = "AI Jobs";

}

<div id="Jobs_page">
    <div id="cent">
        <h2>AI JOBS</h2>
        <p>AI job dataset: Insights AI-Jobs</p>
        <p>Python source for tables and charts: Kaggle</p>
    </div>
</div>
<div id="data_holder">
    <div id="data_set">
        <div class="data">
            <h2>Top 5 companies</h2>
            <p>according to number of employees</p>
        </div>
        <div>
            <div id="table-container">@Html.Raw(ViewBag.TableHtml)</div>
        </div>
    </div>
    <div id="data_set">
        <div class="data">
            <h2>Job Distribution</h2>
            <p>among top 10 in demand job title</p>
        </div>
        <div>
            <div id="pie-chart-container">@Html.Raw(ViewBag.PieHtml)</div>
        </div>
        
    </div>
    <div id="salaries">
        <div class="data">
            <h2>
                Salaries
            </h2>
            <p>in the last 3 years</p>
        </div>
        <div>
            <div id="chart-container">@Html.Raw(ViewBag.ChartHtml)</div>
    </div>
</div>



</div>

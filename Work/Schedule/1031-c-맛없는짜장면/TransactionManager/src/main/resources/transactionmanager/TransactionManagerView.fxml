<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="transactionmanager.TransactionManagerController" alignment="TOP_CENTER"
      prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
            <children>
                <Button fx:id="openBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0" text="OPEN ACCOUNT">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="closeBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                        text="CLOSE ACCOUNT">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
            <children>
                <Button fx:id="depositBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                        text="DEPOSIT MONEY">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="withdrawBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                        text="WITHDRAW MONEY">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
            <children>
                <Button fx:id="printBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                        text="PRINT ACCOUNTS">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="printDetailBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                        text="PRINT ACCOUNTS DETAIL">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
            <children>
                <Button fx:id="updateBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="410.0"
                        text="UPDATE BALANCE">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <TextField fx:id="textField">
            <VBox.margin>
                <Insets top="50.0"/>
            </VBox.margin>
        </TextField>
        <ScrollPane prefHeight="583.0" prefWidth="800.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="181.0" prefWidth="200.0">
                    <children>
                        <TextArea fx:id="textArea" prefHeight="592.0" prefWidth="800.0"/>
                    </children>
                </AnchorPane>
            </content>
            <VBox.margin>
                <Insets top="30.0"/>
            </VBox.margin>
        </ScrollPane>
    </children>
</VBox>
